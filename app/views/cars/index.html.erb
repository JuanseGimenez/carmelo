<% content_for :head do %>
  <%= stylesheet_link_tag 'dashboards' %>
  <%= stylesheet_link_tag 'trips' %>
  <%= stylesheet_link_tag 'cars' %>
<% end %>
<% content_for :title, "Tus Autos" %>
<%= render partial: "dashboards/header/navbar" %>
<div class="container" id="trips-pages">
  <div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      <%= render partial: "shared/flash_messages", flash: flash %>
      <div class="col-lg-4">
        <%= render partial: "dashboards/columns/column-left" %>
      </div>
      <div class="col-lg-8">
      <div class="well" id="index-cars">
        <h2><b>Mis autos</b></h2>
        <div class="row">
          <% @cars.each do |car| %>
            <div class="col-sm-3">
              <div class="card">
                <img class="card-img-top" src="data:image/png;base64," alt="Card image cap">
                <div class="card-block">
                  <h4 class="card-title"><%= car.name %> <%= car.model %></h4>
                  <p class="card-text">Asientos disponibles: <%= car.places %></p>
                  <%= link_to "Ver", car_path(car) , class: "btn btn-primary" %>
                </div>
              </div>
            </div>
          <% end %>
        </div>
        </div>
      </div>
    </div>
  </div>
</div>