      </div>
      <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
        <%= render partial: "shared/flash_messages", flash: flash %>
        <div class="well" id="show-cars">
          <h2><b>Mi  <%= @car.car_with_model %></b></h2>
          <p>Nombre: <%= @car.name %></p>
          <p>Modelo: <%= @car.model %></p>
          <p>Asientos disponibles: <%= @car.places %></p>
